{
  "name": "kg-cli",
  "version": "1.0.0",
  "description": "this is kinggloose private node-cli",
  "scripts": {
    "build": "pnpm -r --no-sort exec npm run build",
    "build:watch": "pnpm -r --no-sort exec npm run build:watch",
    "cs:add": "npm run build && changeset add",
    "cs:version": "changeset version",
    "cs:publish": "changeset publish --no-git-tag"
  },
  "keywords": [
    "cli"
  ],
  "author": "kinggloose",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@types/node": "^22.13.9",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "typescript": "^5.8.2"
  }
}
